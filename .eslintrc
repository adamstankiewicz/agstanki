{
  "env": {
    "browser": true,
    "node": true
  },
  ecmaFeatures: {
    jsx: true
  },
  "globals": {
    "window": true,
    "document": true
  },
  "plugins": [
  ],
  "extends": "eslint-config-airbnb",
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "id-length": 0,
    "react/prop-types": 0,
    "no-param-reassign": 0,
    "react/no-danger": 0,
    "class-methods-use-this": 0,
    "jsx-a11y/href-no-hash": 0
  }
}
